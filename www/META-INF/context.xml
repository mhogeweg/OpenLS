<?xml version="1.0" encoding="UTF-8"?>
<Context crossContext="true" debug="0" docBase="GPT10" path="/Geoportal" reloadable="true">    
	<Resource
		name="jdbc/gpt"
		auth="Container"
		type="javax.sql.DataSource"
		driverClassName="org.postgresql.Driver"
		url="jdbc:postgresql:Geoportal10"
		username="username"
		password="password"
		maxActive="20"
		maxIdle="10"
		maxWait="-1"/>    
</Context>
